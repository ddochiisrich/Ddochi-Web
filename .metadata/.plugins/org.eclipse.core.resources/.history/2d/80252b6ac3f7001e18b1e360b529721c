document.addEventListener("DOMContentLoaded", function(){
	
	const writeForm = document.querySelector("#writeForm");
	writeForm.addEventListener("submit", () =>{
	// alert("writeForm submit");
	// 판단 - 서버로 전송해도 되는지 판단
	const writer = documentQthis.querySelector("writer").value;
	const title = documentQthis.querySelector("title").value;
	const pass = documentQthis.querySelector("pass").value;
	const content = documentQthis.querySelector("content").value;

		if(writer.trim().length <= 0){
		// 폼을 전송하면 안됨
	}
		if(title.trim().length <= 0){
		// 폼을 전송하면 안됨
	}
		if(pass.trim().length <= 0){
		// 폼을 전송하면 안됨
	}
		if(content.trim().length <= 0){
		// 폼을 전송하면 안됨
	}
	
	});
	
});


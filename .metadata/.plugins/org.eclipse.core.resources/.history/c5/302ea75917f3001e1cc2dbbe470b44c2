<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<% 

	String name = request.getParameter("name");
	String food = request.getParameter("food");
	String colors = request.getParameter("colors");
	String hamster = request.getParameter("hamster");
	

	request.setAttribute("name", name);
	request.setAttribute("food", food);
	request.setAttribute("colors", colors);
	request.setAttribute("hamster", hamster);

	
	response.sendRedirect("ch0501ExamResult.jsp?name="+name+"&food="+food+"&colors="+colors+"&hamster="+hamster);
%>

	<script>
		location.href="ch0501ExamResult.jsp";
	</script>
